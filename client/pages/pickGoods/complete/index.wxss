.container {
    width: 100%;
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
    background-color: #ffffff;
}

.container-icon {
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-around;
}

.complete-icon {
    width: 64px;
    height: 64px;
    margin-top: 60px;
}

.complete-text {
    font-size: 20px;
    color: #333;
    margin-top: 30px;
}

.complete-hint-text {
    font-size: 16px;
    color: #ff0000;
    margin: 10px 30px 50px 30px;
}